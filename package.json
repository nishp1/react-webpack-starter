{
  "name": "webpack-starter",
  "version": "0.0.1",
  "description": "Webpack Starter",
  "main": "",
  "scripts": {
    "dev-server": "mkdir -p dist && webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline",
    "hot-dev-server": "mkdir -p dist && webpack-dev-server --config webpack-hot-dev-server.config.js --progress --colors --port 2992 --inline",
    "build": "mkdir -p dist && webpack --config webpack-production.config.js --progress --profile --colors",
    "start": "http-server ./dist"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "0.12.x"
  },
  "dependencies": {
    "babel-core": "^5.0.7",
    "babel-loader": "^5.0.0",
    "bootstrap-sass": "^3.3.4",
    "connect": "^3.3.5",
    "css-loader": "^0.9.1",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^0.5.0",
    "file-loader": "^0.8.1",
    "flummox": "^3.5.0",
    "font-awesome": "^4.3.0",
    "html-loader": "^0.2.3",
    "jquery": "^2.1.3",
    "node-sass": "3.0.0-alpha.0",
    "object-assign": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react": "^0.13.1",
    "react-hot-loader": "^1.2.4",
    "react-router": "^0.13.2",
    "sass-loader": "1.0.0",
    "style-loader": "0.9.0",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack-dev-server": "^1.7.0"
  },
  "devDependencies": {
    "webpack": "^1.7.3"
  }
}
